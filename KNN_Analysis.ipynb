   "outputs": [],
   "source": [
    "categorical_features = [\"marital-status\", \"relationship\", \"occupation\", \"workclass\", \"race\"]\n",
    "binary_features = [\"sex\"]\n",
    "drop_features = [\"age\", \"fnlwgt\", \"education\", \"education-num\", \"capital-gain\", \"capital-loss\", \"hours-per-week\", \"native-country\"]"
   ]
  },
